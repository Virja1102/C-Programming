Q1

#include <stdio.h>
 
int main() {
    int start = 41; 

    
    int rows = 5;

    
    for (int i = 1; i <= rows; i++) {
       
        for (int j = 0; j < i; j++) {
            printf("%d ", start + j);
        }
        printf("\n");
    }

    return 0;
}


Q2


#include <stdio.h>

int main() {
    int num = 11; 
 int rows = 4;

    
    for (int i = 1; i <= rows; i++) {
        
        for (int j = 1; j <= i; j++) {
            printf("%d ", num);
            num++; 
        }
        printf("\n");
    }

    return 0;
}


Q3


#include <stdio.h>

int main() {
    int start = 5; 
    int rows = 5;

     
    for (int i = 0; i < rows; i++) {
        
        for (int j = start - i; j <= start; j++) {
            printf("%d ", j);
        }
        printf("\n");
    }

    return 0;
}


Q4


#include <stdio.h>

int main() {
    
    int rows = 5;

    
    for (int i = 0; i < rows; i++) {
         
        for (int j = 0; j < rows - i; j++) {
            
            if (j % 2 == 0) {
                printf("1 ");
            } else {
                printf("0 ");
            }
        }
        printf("\n");
    }

    return 0;
}


Q5


#include <stdio.h>

int main() {
    int maxNumber = 5; 

    
    for (int i = 1; i <= maxNumber; i++) {
        
        for (int j = 1; j <= maxNumber - i; j++) {
            printf(" ");
        }

        
        for (int j = i; j >= 1; j--) {
            printf("%d ", j);
        }

        
        for (int j = 2; j <= i; j++) {
            printf("%d ", j);
        }

        printf("\n");
    }

    return 0;
}


Q6


#include <stdio.h>

int main() {
    int rows = 5;

    
    for (int i = 1; i <= rows; i++) {
        
        for (int j = 1; j <= i; j++) {
            printf("%d ", j);}

        
        for (int j = i; j >= 1; j--) {
            printf("%d ", j);
        }

        printf("\n");
    }

    return 0;
}


Q7


#include <stdio.h>

int main() {
    
    int rows = 5;

    
    for (int i = 1; i <= rows; i++) {
        if (i == 1 || i == 3) {
            
            for (int j = 1; j <= 5; j++) {
                printf("* ");
            }
        } else if (i == 2) {
            
            for (int j = 1; j <= 2; j++) {
                printf("* ");
            }
        } else if (i == 4 || i == 5) {
           
            printf("* ");
        } printf("\n");
    }

    return 0;
}
